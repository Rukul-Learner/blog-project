<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label for="titlePost">Title</label>
        <input type="text" class="form-control" id="titlePost" name="postTitle">
        </div>
<div class="form-group">
    <label for="post">Post</label>
        <textarea name="postBody" id="post" cols="30" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
